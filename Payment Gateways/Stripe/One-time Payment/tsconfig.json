{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "public/dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
